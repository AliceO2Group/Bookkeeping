  "devDependencies": {
    "chai": "4.2.0",
    "chai-openapi-response-validator": "0.9.4",
    "eslint": "7.11.0",
    "js-yaml": "3.14.0",
    "mocha": "8.1.3",
    "nodemon": "2.0.4",
    "nyc": "15.1.0",
    "puppeteer": "5.3.1",
    "puppeteer-to-istanbul": "git+https://github.com/SoftwareForScience/puppeteer-to-istanbul.git",
    "supertest": "5.0.0"
