{
  "name": "@aliceo2/bookkeeping",
  "version": "0.92.0",
  "author": "ALICEO2",
  "scripts": {
    "coverage": "nyc npm test && npm run coverage:report",
    "coverage:report": "nyc report --reporter=html --reporter=json",
    "lint": "eslint . --ext .js",
    "lint:fix": "npm run lint -- --fix",
    "sequelize": "sequelize-cli",
    "start:dev": "nodemon --ignore 'lib/public/**/*.js' lib/main.js",
    "start:prod": "node lib/main.js",
    "test": "mocha --exit --timeout 0",
    "test:unit": "nyc -- mocha --exit --timeout 0 test/scripts/test-unit.js && nyc report --report-dir=/usr/src/app/coverage/unit --reporter=json",
    "test:api": "nyc -- mocha --exit --timeout 0 test/scripts/test-api.js && nyc report --report-dir=/usr/src/app/coverage/api --reporter=json",
    "test:lhcPeriods": "nyc -- mocha --exit --timeout 0 test/scripts/test-lhcPeriods.js && nyc report --report-dir=/usr/src/app/coverage/lhcPeriods --reporter=json",
    "test:lhcFills": "nyc -- mocha --exit --timeout 0 test/scripts/test-lhcFills.js && nyc report --report-dir=/usr/src/app/coverage/lhcFills --reporter=json",
    "test:logs": "nyc -- mocha --exit --timeout 0 test/scripts/test-logs.js && nyc report --report-dir=/usr/src/app/coverage/logs --reporter=json",
    "test:envs": "nyc -- mocha --exit --timeout 0 test/scripts/test-envs.js && nyc report --report-dir=/usr/src/app/coverage/envs --reporter=json",
    "test:runs": "nyc -- mocha --exit --timeout 0 test/scripts/test-runs.js && nyc report --report-dir=/usr/src/app/coverage/runs --reporter=json",
    "test:subsystems": "nyc -- mocha --exit --timeout 0 test/scripts/test-subsystems.js && nyc report --report-dir=/usr/src/app/coverage/subsystems --reporter=json",
    "test:tags": "nyc -- mocha --exit --timeout 0 test/scripts/test-tags.js && nyc report --report-dir=/usr/src/app/coverage/tags --reporter=json",
    "test:flp": "nyc -- mocha --exit --timeout 0 test/scripts/test-flps.js && nyc report --report-dir=/usr/src/app/coverage/flp --reporter=json",
    "test:home": "nyc -- mocha --exit --timeout 0 test/scripts/test-home.js && nyc report --report-dir=/usr/src/app/coverage/home --reporter=json",
    "test:about": "nyc -- mocha --exit --timeout 0 test/scripts/test-about.js && nyc report --report-dir=/usr/src/app/coverage/about --reporter=json",
    "test:eosReport": "nyc -- mocha --exit --timeout 0 test/scripts/test-eosReport.js && nyc report --report-dir=/usr/src/app/coverage/eosReport --reporter=json",
    "test:dataPasses": "nyc -- mocha --exit --timeout 0 test/scripts/test-dataPasses.js && nyc report --report-dir=/usr/src/app/coverage/dataPasses --reporter=json",
    "test:simulationPasses": "nyc -- mocha --exit --timeout 0 test/scripts/test-simulationPasses.js && nyc report --report-dir=/usr/src/app/coverage/simulationPasses --reporter=json",
    "test:qcFlagTypes": "nyc -- mocha --exit --timeout 0 test/scripts/test-qcFlagTypes.js && nyc report --report-dir=/usr/src/app/coverage/qcFlagTypes --reporter=json",
    "test:qcFlags": "nyc -- mocha --exit --timeout 0 test/scripts/test-qcFlags.js && nyc report --report-dir=/usr/src/app/coverage/qcFlags --reporter=json",
    "docker-run": "docker compose -f docker-compose.yml -f docker-compose.dev.yml up --build",
    "docker-test": "docker compose -p test -f docker-compose.yml -f docker-compose.test.yml up --build --abort-on-container-exit",
    "docker-test:parallel": "docker compose -p test-parallel -f docker-compose.test-parallel.yml up --build",
    "docker-update": "node scripts/update-dockerfile.js"
  },
  "engines": {
    "node": ">= 18.x"
  },
  "dependencies": {
    "@aliceo2/web-ui": "2.6.0",
    "@grpc/grpc-js": "1.10.9",
    "@grpc/proto-loader": "0.7.0",
    "cls-hooked": "4.2.2",
    "d3": "7.8.5",
    "deepmerge": "4.3.0",
    "joi": "17.13.1",
    "mariadb": "3.0.0",
    "multer": "1.4.5-lts.1",
    "node-fetch": "3.3.1",
    "sequelize": "6.37.0",
    "umzug": "3.8.0"
  },
  "files": [
    "lib/",
    "docs/",
    "proto/"
  ],
  "devDependencies": {
    "@types/d3": "7.4.0",
    "chai": "4.4.1",
    "date-and-time": "3.3.0",
    "eslint": "8.57.0",
    "js-yaml": "4.1.0",
    "mocha": "10.5.1",
    "nodemon": "3.1.3",
    "nyc": "17.0.0",
    "puppeteer": "22.12.0",
    "puppeteer-to-istanbul": "1.4.0",
    "sequelize-cli": "6.6.0",
    "sinon": "18.0.0",
    "supertest": "7.0.0"
  },
  "bundleDependencies": [
    "@aliceo2/web-ui",
    "@grpc/grpc-js",
    "@grpc/proto-loader",
    "cls-hooked",
    "d3",
    "deepmerge",
    "joi",
    "mariadb",
    "multer",
    "node-fetch",
    "sequelize",
    "umzug"
  ]
}
