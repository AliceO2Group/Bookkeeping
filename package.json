    "umzug": "2.3.0"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "chai-openapi-response-validator": "0.11.0",
    "eslint": "7.14.0",
    "js-yaml": "3.14.0",
    "mocha": "8.2.1",
    "nodemon": "2.0.6",
    "nyc": "15.1.0",
    "puppeteer": "5.5.0",
    "puppeteer-to-istanbul": "git+https://github.com/SoftwareForScience/puppeteer-to-istanbul.git",
    "supertest": "6.0.1"
