{
  "name": "@aliceo2/bookkeeping",
  "version": "0.71.0",
  "author": "ALICEO2",
  "scripts": {
    "coverage": "nyc npm test && npm run coverage:report",
    "coverage:report": "nyc report --reporter=html --reporter=json",
    "lint": "eslint . --ext .js",
    "lint:fix": "npm run lint -- --fix",
    "sequelize": "sequelize-cli",
    "start:dev": "nodemon --ignore 'lib/public/**/*.js' lib/main.js",
    "start:prod": "node lib/main.js",
    "test": "mocha --exit --timeout 0 --grep LhcPeriodStatisticsService",
    "docker-run": "docker compose -f docker-compose.yml -f docker-compose.dev.yml up --build",
    "docker-test": "docker compose -p test -f docker-compose.yml -p test -f docker-compose.test.yml up --build --abort-on-container-exit"
  },
  "engines": {
    "node": ">= 18.x"
  },
  "dependencies": {
    "@aliceo2/web-ui": "2.5.0",
    "@grpc/grpc-js": "1.9.0",
    "@grpc/proto-loader": "0.7.0",
    "cls-hooked": "4.2.2",
    "d3": "7.8.5",
    "deepmerge": "4.3.0",
    "joi": "17.11.0",
    "mariadb": "3.0.0",
    "multer": "1.4.5-lts.1",
    "node-fetch": "3.3.1",
    "sequelize": "6.34.0",
    "umzug": "3.4.0"
  },
  "files": [
    "lib/",
    "docs/",
    "proto/"
  ],
  "devDependencies": {
    "@types/d3": "7.4.0",
    "chai": "4.3.4",
    "eslint": "8.51.0",
    "js-yaml": "4.1.0",
    "mocha": "10.2.0",
    "nodemon": "3.0.1",
    "nyc": "15.1.0",
    "puppeteer": "21.3.4",
    "puppeteer-to-istanbul": "1.4.0",
    "sequelize-cli": "6.6.0",
    "sinon": "16.1.0",
    "supertest": "6.3.0"
  },
  "bundleDependencies": [
    "@aliceo2/web-ui",
    "cls-hooked",
    "deepmerge",
    "joi",
    "mariadb",
    "multer",
    "sequelize",
    "umzug"
  ]
}
